import java.util.*;
public class Main {
	public static void printDec(int n) {
		if(n==1) {
			System.out.print(1);
			return;
		}
		System.out.print(n+" ");
		printDec(n-1);
	}
	
	public static void main(String[] args) {
		Scanner sc=new Scanner(System.in);
		int n=sc.nextInt();
		printDec(n);
	}
}
